<h1>Listing mailee_contacts</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Company</th>
    <th>Position</th>
    <th>Phone</th>
    <th>Address</th>
    <th></th>
  </tr>

<% @mailee_contacts.each do |mailee_contact| %>
  <tr>
    <td><%= mailee_contact.name %></td>
    <td><%= mailee_contact.email %></td>
    <td><%= mailee_contact.company %></td>
    <td><%= mailee_contact.position %></td>
    <td><%= mailee_contact.phone %></td>
    <td><%= mailee_contact.address %></td>
    <td><%= link_to 'Show', mailee_contact %></td>
  </tr>
<% end %>
</table>

<br />
